<faces-config
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
      http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
  version="2.2">
  <navigation-rule>
    <from-view-id>/graph.xhtml</from-view-id>
    <navigation-case>
      <description>Navigate to history</description>
      <from-outcome>history</from-outcome>
      <to-view-id>/history.xhtml</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <from-view-id>/history.xhtml</from-view-id>
    <navigation-case>
      <description>Navigate to graph</description>
      <from-outcome>graph</from-outcome>
      <to-view-id>/graph.xhtml</to-view-id>
      <redirect />
    </navigation-case>
  </navigation-rule>
</faces-config>

